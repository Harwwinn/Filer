{
  "expo": {
    "name": "Filer",
    "slug": "Filer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/filerLogo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/filerLogo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.juanincho231.Filer"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/filerLogo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c86f93f5-9bc4-4840-a64a-11ceeb49ab6d"
      }
    }
  }
}
